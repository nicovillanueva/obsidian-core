{
    "path": "archive/$date/$url",
    "filename": "$browser.png",
    "store_in_db": true,
    
    "urls": [
        "http://www.viajeros.com/",
        "http://www.viajeros.com/destinos",
        "http://www.viajeros.com/hoteles/hoteles.htm"
    ],
